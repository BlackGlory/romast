{
  "name": "romast",
  "version": "3.0.0",
  "description": "Renderable Org-Mode Abstract Syntax Tree",
  "keywords": [],
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/es2018/index.js",
  "exports": {
    ".": "./lib/es2018/index.js",
    "./utils/*": "./lib/es2018/romast-utils/*",
    "./es2018": "./lib/es2018/index.js",
    "./es2015": "./lib/es2015/index.js",
    "./es2018/utils/*": "./lib/es2018/romast-utils/*",
    "./es2015/utils/*": "./lib/es2015/romast-utils/*"
  },
  "types": "lib/es2018/index.d.ts",
  "repository": "git@github.com:BlackGlory/romast.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --no-cache --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "prepublishOnly": "run-s clean build bundle",
    "clean": "run-p clean:*",
    "clean:build": "rimraf lib",
    "clean:bundle": "rimraf dist",
    "build": "run-p build:*",
    "build:es2015": "run-s build:es2015:*",
    "build:es2015:compile": "tsc --project tsconfig.build.json --target es2015 --outDir lib/es2015",
    "build:es2015:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2015",
    "build:es2018": "run-s build:es2018:*",
    "build:es2018:compile": "tsc --project tsconfig.build.json --target es2018 --outDir lib/es2018",
    "build:es2018:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2018",
    "bundle": "rollup --config rollup.config.js",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-matchers": "^0.2.1",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.0.3",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.droprightwhile": "^4.6.6",
    "@types/lodash.dropwhile": "^4.6.6",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "extra-tags": "^0.2.3",
    "husky": "4",
    "jest": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.1",
    "tscpaths": "^0.0.9",
    "tslib": "^2.3.1",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "@blackglory/errors": "^1.1.2",
    "@blackglory/types": "^0.6.0",
    "@types/unist": "^2.0.6",
    "core-js": "^3.20.0",
    "date-fns": "^2.27.0",
    "hotypes": "^0.4.3",
    "iterable-operator": "^0.14.4",
    "lodash.clonedeep": "^4.5.0",
    "lodash.droprightwhile": "^4.6.0",
    "lodash.dropwhile": "^4.6.0",
    "nanoid": "^3.1.30",
    "orga": "3.2.0"
  }
}
